<?xml version="1.0"?>
<launch>

	<!-- send table urdf to param server -->
	<param name="mug_description" command="$(find xacro)/xacro.py $(find gazebo_worlds)/objects/mug-test.urdf" />

	<!-- push table_description to factory and spawn robot in gazebo -->
	<node name="spawn_mug" pkg="gazebo" type="spawn_model" args="-urdf -param mug_description -x -0.8 -y -0.2 -z 1.1 -R 0.0 -P 0.0 -Y -0.7 -model mug_model" respawn="false" output="screen" />

</launch>
