<?xml version="1.0"?>
<launch>

	<!-- send table urdf to param server -->
	<param name="tomato_soup_description" command="$(find xacro)/xacro.py $(find cob_gazebo_objects)/objects/tomato_soup.urdf" />

	<!-- push table_description to factory and spawn robot in gazebo -->
	<node name="spawn_tomato_soup" pkg="gazebo" type="spawn_model" args="-urdf -param tomato_soup_description -x -0.8 -y -0.2 -z 1.1 -R 0.0 -P 0.0 -Y -0.7 -model tomato_soup_model" respawn="false" output="screen" />

</launch>
